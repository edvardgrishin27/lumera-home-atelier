# SHA-256 hash of admin password
# Generate: echo -n "YourPassword" | shasum -a 256
VITE_ADMIN_PASSWORD_HASH=your_sha256_hash_here

# Secret UUID for admin panel URL (e.g., /panel/{UUID}/login)
# Generate: python3 -c "import uuid; print(uuid.uuid4())"
VITE_ADMIN_UUID=your_uuid_here

# TOTP secret for 2FA (Base32 encoded)
# Generate: python3 -c "import secrets,base64; print(base64.b32encode(secrets.token_bytes(20)).decode().rstrip('='))"
VITE_TOTP_SECRET=your_totp_secret_here
